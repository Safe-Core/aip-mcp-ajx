{
  "name": "@safecore/aip-mcp-ajx",
  "version": "1.1.0",
  "description": "Busca de dados de controle de acesso em condom√≠nios usando Qdrant e rastreamento integrado com Firestore.",
  "main": "dist/index.js",
  "type": "module",
  "bin": {
    "aip-mcp-ajx": "dist/index.js"
  },
  "scripts": {
    "build": "tsc",
    "dev": "tsx watch src/index.ts",
    "start": "node dist/index.js",
    "inspect": "npx @modelcontextprotocol/inspector node dist/index.js",
    "inspect:dev": "npx @modelcontextprotocol/inspector tsx src/index.ts",
    "prepare-indices": "node dist/criar-indices.js",
    "clean": "rimraf dist",
    "type-check": "tsc --noEmit",
    "test": "tsx src/test.ts",
    "test:rastreamento": "tsx tests/teste-rastreamento-visitante.ts",
    "test:tokensteps": "tsx tests/teste-token-steps.ts",
    "test:visitante": "tsx tests/teste-buscar-rastreamento-visitante.ts",
    "examples": "tsx src/examples.ts",
    "prepare": "npm run clean && npm run build"
  },
  "keywords": [
    "mcp",
    "server",
    "condominio",
    "access-control",
    "qdrant",
    "agnos"
  ],
  "author": "Safe Core",
  "license": "ISC",
  "dependencies": {
    "@modelcontextprotocol/sdk": "^0.5.0",
    "@qdrant/js-client-rest": "^1.10.0",
    "dotenv": "^16.4.5",
    "firebase": "^12.0.0",
    "openai": "^4.104.0",
    "zod": "^3.23.8"
  },
  "devDependencies": {
    "@types/node": "^20.14.0",
    "rimraf": "^5.0.7",
    "tsx": "^4.16.0",
    "typescript": "^5.5.0"
  }
}
